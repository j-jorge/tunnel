sprite_sheet "forest-2" 1024 x 1024 margin 1 order "height"

tree "tree/tree.xcf.bz2"
ground "ground/ground-grass.xcf.bz2"
wall "wall/base-block.xcf.bz2"
flower "flower/flower.xcf.bz2"

"left branch" autosize * 0.5 with tree
  "left"
  ;

"grass 1" autosize * 0.5 with ground
  [ hollow ] "top grass alt color"
  [ hollow ] "hard lights 1"
  [] "1-1"
  "1-2"
  "1-3"
  ;

"grass 2" autosize * 0.5 with ground
  [ hollow ] "top grass alt color"
  [ hollow ] "hard lights 1"
  "1-1"
  "1-2"
  ;

"grass 3" autosize * 0.5 with ground
  [ hollow ] "top grass alt color"
  [ hollow ] "hard lights 1"
  "1-1"
  "1-3"
  ;

"grass 4" autosize * 0.5 with ground
  [ hollow ] "top grass alt color"
  [ hollow ] "hard lights 1"
  "1-1"
  ;

"grass 5" "2" * 0.5 with ground
  "2"
  ;

"leaves 6" "4" * 0.5 with ground
  "4"
  ;

"wall fill left" "left mask" * 0.5 with wall
  "common"
  "center"
  mask "left mask"
  ;

"wall fill bottom left" "bottom left mask" * 0.5 with wall
  "common"
  "center"
  mask "bottom left mask"
  ;

"wall fill bottom" "bottom mask" * 0.5 with wall
  "common"
  "center"
  mask "bottom mask"
  ;

"wall fill middle" "middle mask" * 0.5 with wall
  "common"
  "center"
  mask "middle mask"
  ;

"wall fill middle 2" "middle mask" * 0.5 with wall
  "common"
  "center 2"
  mask "middle mask"
  ;

"flower 1-1" "flower 1" * 0.5 with flower
  "flower head 1"
  "flower bottom 1"
  "1-1"
  ;

"flower 1-2" "flower 1" * 0.5 with flower
  "flower head 2"
  "flower bottom 2"
  "1-2"
  ;

"flower 1-3" "flower 1" * 0.5 with flower
  "flower head 3"
  "flower bottom 3"
  "1-3"
  ;

"flower 1-4" "flower 1" * 0.5 with flower
  "flower head 4"
  "flower bottom 4"
  "1-4"
  ;
